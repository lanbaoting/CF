        var sectionAsideGameList = ViewBag.SectionAsideGameList as List<Entity.ViewModels.DjSectionAsideGame>;
       

        if (sectionList != null)
        {
            for (int i = 0; i < sectionList.Count; i++)
            {


                if (sectionCoreList != null)
                {
                    var sectionCore1 = sectionCoreList.Find(w => w.DjSectionId == sectionList[i].Id);
                    if (sectionCore1 != null)
                    {


                        <section id="jwtab-@(i+1)" class="box-one">
                                <header class="mod-hd hd-one tab-hd hd">
                                    @{
                                        if (sectionCoreItemList != null)
                                        {
                                            <ul>

                                                @{
                                                    var sectionCoreItems = sectionCoreItemList.Where(w => w.DjSectionCoreId == sectionCore1.Id).ToList();
                                                    for (int i1 = 0; i1 < sectionCoreItems.Count; i1++)
                                                    {
                                                        if (i1 == 0)
                                                        {
                                                            <li class="on">@sectionCoreItems[i1].Title</li>
                                                        }
                                                        else
                                                        {
                                                            <li>@sectionCoreItems[i1].Title</li>
                                                        }
                                                    }
                                                }

                                            </ul>
                                            <h2><i class="ft1">L</i>@sectionCore1.Title</h2>
                                        }
                                    }
                                </header>
                                <div class="bd">                                     
                                    @{
                                        if (sectionCoreItemList != null)
                                        {
                                            var sectionCoreItems = sectionCoreItemList.Where(w => w.DjSectionCoreId == sectionCore1.Id).ToList();
                                            for (int i1 = 0; i1 < sectionCoreItems.Count; i1++)
                                            {
                                                string classStr = "class=\"ds-n\"";

                                                if (sectionCoreItemGameList != null)
                                                {
                                                    <article @classStr>
                                                        <ul class="mod-one mod-seven">
                                                            @{
                                                                var sectionCoreItemGames = sectionCoreItemGameList.Where(w => w.DjSectionCoreItemId == sectionCoreItems[i1].Id);
                                                                foreach (var m in sectionCoreItemGames)
                                                                {
                                                                    <li>
                                                                        <a href="/Game/@(m.DjGameId).html">
                                                                            <img src="~/images/lgImg.gif" data-original="@m.CoverMap" original="@m.CoverMap" alt="@m.Name" title="@m.Name" />
                                                                            <span>@m.Name</span>
                                                                            <p class="down">下载</p>
                                                                        </a>
                                                                    </li>
                                                                }
                                                            }
                                                        </ul>
                                                    </article>    
                                                    classStr = "";
                                                }
                                            }
                                        }
                                    }
                                </div>
                        </section>
                    }
                    if (sectionAsideList != null)
                    {
                        var sectionAside1 = sectionAsideList.Find(w => w.DjSectionId == sectionList[i].Id);
                        if (sectionAside1 != null)
                        {
                            <section class="box-one">
                                <header class="mod-hd hd-one">
                                    <h2><i class="ft1">K</i>@sectionAside1.Title</h2>
                                </header>
                                <ul class="mod-one mod-seven">
                                    @{
                                        if (sectionAsideGameList != null)
                                        {
                                            var sectionAsideGames = sectionAsideGameList.Where(w => w.DjSectionAsideId == sectionAside1.Id);
                                            foreach (var m in sectionAsideGames)
                                            {                                                
                                                <li>
                                                    <a href="//sjyjzx.cn/game/34138.html">
                                                        <img src="~/images/lgImg.gif" data-original="@m.Name" />
                                                        <span>@m.Name</span>
                                                        <p class="down">下载</p>
                                                    </a>
                                                </li>
                                            }
                                        }
                                    }                                     
                                </ul>
                            </section>
                        }
                    }
                }
            }
        }
    }

 
