    @{
        var sectionList = ViewBag.SectionList as List<Entity.Models.Jw_DjSection>;

        var sectionCoreList = ViewBag.SectionCoreList as List<Entity.Models.Jw_DjSectionCore>;

        var sectionCoreItemList = ViewBag.SectionCoreItemList as List<Entity.Models.Jw_DjSectionCoreItem>;

