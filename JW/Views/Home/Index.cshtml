        <ul class="pic_list_tel">
            @{
                var factoryHouseChuShouList = ViewBag.FactoryHouseChuShouList as List<Entity.Models.Cf_FactoryHouse>;
                foreach (var m in factoryHouseChuShouList)
                {
                    <li>
                        <a href="/changfang/@(m.Id).html" title="@m.HousingName" target="_blank">
                            <img class="lazy" src="@m.CoverMap" alt="@m.HousingName" width="226" height="170" />
                        </a>
                        <div class="con_bt_p">
                            <h4><a href="/changfang/@(m.Id).html" target="_blank" style=" ">@m.HousingName</a></h4>
                            <div class="con_bt_p_mx">
                                <div class="fl"> <span>@(m.UnitPrice == null ? "面议" : m.UnitPrice.ToString()) </span> 元/平 </div>
                                <div class="fr">@m.HousingArea 平</div>
                            </div>
                            <div class="con_bt_p_mx">
                                <div class="fl">@m.F_CreateDate.Value.ToString("yyyy-MM-dd")</div>
                                <div class="fr">已浏览 @m.Pageviews</div>
                            </div>
                        </div>
                    </li>
                }
            }
        </ul>
    </div>
    <div class="w1200 con con_6">
        <div class="con_bt"><h3>@(ViewBag.Area.Name)厂房最近出租</h3><a class="more" href="/shangpuchushou/" title="厂房最近出租">厂房最近出租;</a></div>
