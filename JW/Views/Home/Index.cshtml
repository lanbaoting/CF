                                <div class="fl">@m.F_CreateDate.Value.ToString("yyyy-MM-dd")</div>
                                <div class="fr">已浏览 @m.Pageviews</div>
                            </div>
                        </div>
                    </li>
                }
            }
        </ul>
    </div>


    <div class="w1200 con con_4">
        <div class="min">
            <div class="con_bt"><h3>按结类别找厂房</h3><a class="more" href="/shangpuzhuanrang/" title="更多北京商铺转让信息" target="_blank">更多@(ViewBag.Area.Name)厂房信息</a></div>
            <div class="tab">
                <ul class="title" id="ul_typeshopinfo">
                    @{

                        var dictionarysJieGous = ViewBag.DictionarysJieGous as List<Entity.Models.Cf_FactoryHouseDictionary>;
                        int i = 0;
                        foreach (var m in dictionarysJieGous)
                        {
                            i++;
                            if (i == 0)
                            {
                                <li typeid="@i" class="first on">@m.Name</li>
                            }
                            else
                            {
                                <li typeid="@i">@m.Name</li>
                            }
                        }
                    }
                </ul>
                <div class="tab_con">
                    @{
                        var housingStructureHouseList = ViewBag.HousingStructureHouseList as List<Entity.ViewModels.GroupFactoryHouseDictionaryHouse>;
                        i = 0;
                        string style = "block";
                        foreach (var m in housingStructureHouseList)
                        {

                            i++;
                            <div class="icontent div_typeshopinfo" typeid="@i" style="display:@style;">
                                <ul class="list">
                                    @{

                                        foreach (var h in m.FactoryHouseList)
                                        {
                                            <li>
                                                <span class="areaname">
                                                    <a href="/changfang/@(h.Id).html" target="_blank">@h.DistrictName</a>
                                                </span>
                                                <a href="/changfang/@(h.Id).html" title="@h.HousingName" target="_blank" style=" ">@h.HousingName</a>
                                                <span class="price">单价 <em>@(h.UnitPrice==null? "面议" : h.UnitPrice.ToString())</em></span>
                                            </li>
                                        }
                                    }
                                </ul>
                                <h4 class="more"><a href="/jiulou/" title="@m.FactoryHouseDictionaryName">更多@(m.FactoryHouseDictionaryName)信息 &gt;&gt;</a></h4>
                            </div>
                            style = "none";
                        }
                    }
                </div>
            </div>
        </div>
    </div>











    <div class="con_7 w1200 con">
        @{
            var factoryHouseDictionaryHouseList = ViewBag.FloorTypeFactoryHouseList as List<Entity.ViewModels.GroupFactoryHouseDictionaryHouse>;
            string[] classAee = new[] { "l", "m", "r" };
            for (var x = 0; x < factoryHouseDictionaryHouseList.Count; x++)
            {
                if (x > 2)
                {
                    break;
                }
                <div class="@classAee[x]">
                    <div class="con_bt_2"><h3><a href="/cangku/" title="@factoryHouseDictionaryHouseList[x].FactoryHouseDictionaryName">@factoryHouseDictionaryHouseList[x].FactoryHouseDictionaryName</a></h3></div>
                    <ul class="list">
                        @{
                            foreach (var m in factoryHouseDictionaryHouseList[x].FactoryHouseList)
                            {

                                <li>
                                    <span class="catname">
                                        @{
                                            var dic = dictionarysJieGous.Find(w => w.Id == m.HousingStructureId);
                                            if (dic != null)
                                            {
                                                <a href="cangkuchuzu/" target="_blank">@dic.Name</a>
                                            }
                                        }
                                    </span>
                                    <a href="/changfang/@(m.Id).html" target="_blank" style="width: 245px;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;" title="@(m.HousingName)">@(m.HousingName)</a>
                                    @{
                                        if (m.TransactionModeId == 109)
                                        {
                                            <span><em>@(m.HousingRent == null? "": m.HousingRent.ToString())</em> 元/月</span>
                                        }
                                        else
                                        {
                                            <span><em>@(m.UnitPrice == null ? "" : m.UnitPrice.ToString())</em> 元/㎡</span>
                                        }
                                    }
                                </li>
                            }
                        }
                    </ul>
                </div>
            }
        }
    </div>
