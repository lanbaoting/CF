    <div class="nav_bg">
        <div class="nav_main w1200 clear">
            <ul>

                @{
                    var navigationList = ViewBag.NavigationList as List<Entity.Models.Cf_Navigation>;
                    if (navigationList != null)
                    {
                        foreach (var m in navigationList)
                        {
                            if (m.LinkAddress.Contains(ViewBag.Path))
                            {

                                if (m.Title == "首页")
                                {
                                    <li><a href="@m.LinkAddress" class="li_on">@m.Title</a></li>
                                }
                                else
                                {
                                    <li><a target="_self" href="@m.LinkAddress" class="li_on">@m.Title</a></li>
                                }
                            }
                            else
                            {
                                if (m.Title == "首页")
                                {
                                    <li><a href="@m.LinkAddress">@m.Title</a></li>
                                }
                                else
                                {
                                    <li><a target="_self" href="@m.LinkAddress">@m.Title</a></li>
                                }
                            }
                        }
                    }
                }
            </ul>
        </div>
    </div>
    <div class="w1200 con_1 ">
        <div class="tab tab_l">
            <ul class="title" id="ul_typeregion">
                <li class="first on">@(ViewBag.Area.Name)厂房出售</li>
                <li class="">@(ViewBag.Area.Name)厂房出租</li>         
            </ul>
            <div class="tab_con" id="div_tab_con">
                <div class="icontent active">
                    <ol>
                        <li class="ys1">
                            @{
                                string ps = "".PadRight(ViewBag.CitySiteRangeSearchCount, '0').Replace("0", "_0") + "_1";
                                var areaList = ViewBag.AreaList as List<Entity.Models.Cf_FactoryHouseDictionary>;
                                if (areaList != null)
                                {
                                    foreach (var m in areaList)
                                    {
                                        <a href='@ViewBag.Area.Code/chushou@(ps).html' title="@(m.Name)厂房出售">@(m.Name)厂房出售</a>
                                    }
                                }
                            }
                        </li>
                        <li class="ys2">
                            @{
                                var teseHousingList = ViewBag.TeseHousingList as List<Entity.Models.Cf_FactoryHouseDictionary>;
                                if (teseHousingList != null)
                                {
                                    foreach (var m in teseHousingList)
                                    {
                                        if (m.Name.Contains("房"))
                                        {
                                            <a href='@ViewBag.Area.Code/tesechushou_@(m.Id)_1.html' title="@m.Name">@(m.Name)出售 </a>
                                        }
                                        else
                                        {
                                            <a href='@ViewBag.Area.Code/tesechuzu_@(m.Id)_1.html' title="@m.Name">@(m.Name)厂房出售 </a>
                                        }
                                    }
                                }
                            }
                        </li>
                    </ol>
                </div>

                <div class="icontent">
                    <ol>
                        <li class="ys1">
                            @{

                                if (areaList != null)
                                {
                                    foreach (var m in areaList)
                                    {
                                        <a href='@ViewBag.Area.Code/chushou@(ps).html' title="@(m.Name)厂房出租">@(m.Name)厂房出租</a>
                                    }
                                }
                            }
                        </li>
                        <li class="ys2">
                            @{
                                if (teseHousingList != null)
                                {
                                    foreach (var m in teseHousingList)
                                    {
                                        if (m.Name.Contains("房"))
                                        {
                                            <a href='@ViewBag.Area.Code/tesechuzu_@(m.Id)_1.html' title="@m.Name">@(m.Name)出租</a>
                                        }
                                        else
                                        {
                                            <a href='@ViewBag.Area.Code/tesechuzu_@(m.Id)_1.html' title="@m.Name">@(m.Name)厂房出租 </a>
                                        }
                                    }
                                }
                            }
                        </li>
                    </ol>
                </div>
            </div>
        </div>
        <div class="tab_r">
            <div id="tab1">
                <ul>
                    <li onmouseover="setTab(1,0)" class="now">厂房求购</li>
                    <li onmouseover="setTab(1,1)">厂房求组</li>
                    <li onmouseover="setTab(1,2)">热点资讯</li>
                </ul>
            </div>
            <div class="clearfix"></div>
            <div id="tablist1">
                <div class="tablist block corp">
                    <ul>
                        @{
                            var articleList = ViewBag.ArticleList as List<Entity.Models.Jw_Article>;
                            if (articleList != null)
                            {
                                foreach (var m in articleList)
                                {
                                    <li><span class="title"><a target="_blank" href="@m.ArticleTitle" title=">@m.ArticleTitle">@m.ArticleTitle</a></span><span class="time">@m.F_CreateDate.ToString("MM-dd")</span></li>
                                }
                            }
                        }
                    </ul>
                </div>
                <div class="tablist none announce">
                    <ul>
                        @{
                           
                            if (articleList != null)
                            {
                                foreach (var m in articleList)
                                {
                                    <li><span class="title"><a target="_blank" href="@m.ArticleTitle" title=">@m.ArticleTitle">@m.ArticleTitle</a></span><span class="time">@m.F_CreateDate.ToString("MM-dd")</span></li>
                                }
                            }
