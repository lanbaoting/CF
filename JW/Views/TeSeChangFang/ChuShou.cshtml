    <div class="xz_li w1200">
        <!-- 根据当前城市获取区域-->
        @{
            var citySiteRangeSearchList = ViewBag.CitySiteRangeSearchList as List<Entity.Models.Cf_CitySiteRangeSearch>;

            //参数类别
            var conditionalGroupList = citySiteRangeSearchList.Where(w => w.ParentId == 0).ToList();

            string pageUrl = "".PadRight(conditionalGroupList.Count, '0').Replace("0", "_0");

            //地址栏参数

            if (conditionalGroupList != null && citySiteRangeSearchList != null)
            {
                foreach (var m in conditionalGroupList)
                {
                    <div class="select_brand">
                        <span>@m.Name：</span>
                        <div class="a_n" id="xz_2">
                            @{
                                var groupRangeSearchList = citySiteRangeSearchList.Where(w => w.ParentId == m.Id).OrderBy(s => s.Sort).ToList();

                                string params2 = "";
                                foreach (var m2 in conditionalGroupList)
                                {
                                    params2 += "_0";
                                }
                                params2 += "_1";
                                <a href="/chushou@(params2).html" class="class=on">不限</a>

                                foreach (var m2 in groupRangeSearchList)
                                {

                                    string params3 = "";
                                    foreach (var m3 in conditionalGroupList)
                                    {
                                        if (m3.Id == m.Id)
                                        {
                                            params3 += "_" + m2.FactoryHouseDictionaryId;
                                        }
                                        else
                                        {
                                            params3 += "_0";
                                        }
                                    }
                                    params3 += "_1";
                                    <a href="/chushou@(params3).html">@m2.Name</a>
                                }
                            }
                        </div>
                    </div>
                }
            }
        }
